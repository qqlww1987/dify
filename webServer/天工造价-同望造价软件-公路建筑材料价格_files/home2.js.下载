		  methods: {
			changeGlProvince:function(event) {
		      var result=null;
		      if(app.glProviceList.filter){
		    	 result=app.glProviceList.filter(function(item,index,array){
		    		  return item.name==app.glProvice;
		    	  })
		      }else{
		    	  for(var i=0;i<app.glProviceList.length;i++){
		    		  if(app.glProviceList[i].name==app.glProvice){
		    			  result=[app.glProviceList[i]];
		    			  break;
		    		  }
		    	  }
		       }
		      app.glLinkList=result[0].linkList;
		      app.glLink="";
		    },
		    changeJZProvince:function(event){
		    	 var result=null;
			      if(app.jzProviceList.filter){
			    	 result=app.jzProviceList.filter(function(item,index,array){
			    		  return item.name==app.jzProvice;
			    	  })
			      }else{
			    	  for(var i=0;i<app.jzProviceList.length;i++){
			    		  if(app.jzProviceList[i].name==app.jzProvice){
			    			  result=[app.jzProviceList[i]];
			    			  break;
			    		  }
			    	  }
			       }
			      app.jzLinkList=result[0].linkList;
			      app.jzLink="";
		    },    
		   gotoLink:function(event){
			  $("#commonLink").attr("href",app.glLink);
			  $("#commonLink")[0].click();			  
		   },
		   gotoLink2:function(event){
				  $("#commonLink").attr("href",app.jzLink);
				  $("#commonLink")[0].click();			  
		   },
		   dosearchQuestion:function(){
			   $("#commonLink").attr("href","/cms/tgknow/all/all/all/newQ/index-1.html?keyword="+app.searchQuestion);
			   $("#commonLink")[0].click();
		   },
		   dosearchDocument:function(){
			   $("#commonLink").attr("href","/shareData/site/document/search.html?searchWord="+app.searchDocument);
			   $("#commonLink")[0].click();
		   },
		   dosearchVideo:function(){
			   $("#commonLink").attr("href","/shareData/site/document/index.html?selMenu1=005006&queryWord="+app.searchVideo);
			   $("#commonLink")[0].click();
		   },
		   getxmzbLink:function(id){
			 return "/myFiles/myEcCostFile/site/costfile/viewShareEntity.html?id="+id;  
		   },
		   gotoXmzbOrzjShare:function(){
			   
			   var selected=$(".xmal-hd .tit.on").find("span").text();
			   var link="/shareData/composePrice/share/index.html";
			   if(selected.indexOf("项目案例")!=-1){
				   link="/myFiles/myEcCostFile/site/calculate/billZb/xmzbIndex.html";
			   }
			   
			  $("#commonLink").attr("href",link);
			  $("#commonLink")[0].click();	
			   
		   },
		   loadCategoryClList:function(event){
			   $(event.target).addClass("on").siblings().removeClass("on");
			   var categoryName= $(event.target).attr("data-value");
			   dataLoader.loadGovPriceGcCL(categoryName,function(data){
				   layer.close(layer.index);
				   console.log(data); 
				   app.clList=[];
				   for(var j=0;j<data.length;j++){
					   var item=data[j];
					   for(var k=0;k<item.length;k++){
						   app.clList.push(item[k]);
					   }
				   }
				   for(var i=0;i<app.clList.length;i++){
					   var item=app.clList[i];
					   if(item.standard){
				           if(item.model){
				        	   item.viewName=item.name+"["+item.standard+"/"+item.model+"]";
				           }else{
				        	   item.viewName=item.name+"["+item.standard+"]";
				           }
				     }else{
				    	 item.viewName=item.name;
				     }			   
				   }
				   if(app.clList.length>0){
					   app.selectedClId=app.clList[0].id;
					   app.selectedCl=app.clList[0];
					   app.renderClHistory(app.selectedClId);					   
				   }
				   
				   
			  
				   
				   
				   
				   
				   
			   });
		   },
		   
		   renderClHistory:function(detailId){
			   
			   var series_data = "";
				var xArr = [];
				
				function adjuestMaxOrMin(number,isMax){
					var result;
					if(isMax){
						result=number+200;
					}else{
						result=number-200;
				    }
				    var length;
				    if(result.toString().indexOf(".") >= 0) {
					    length=result.toString().indexOf(".");
				    } else {
					    length=result.toString().length;
				    }
				    var multiple="1";
				    for(var i=0;i<length-1;i++){
				 	    multiple=multiple+"0";
				    }
				    multiple=parseInt(multiple);
				    var temp=result/multiple;
				    temp=temp.toFixed(1);
				    temp=parseFloat(temp);
				    var result=temp*multiple;
				    return result;					
				}
				
				
			   $.getJSON("/price/gov/getSixMonthHisPrices.json",{detailId:detailId},function(data) {	
				   if(data.priceList!=null&&data.priceList.length > 0){
						series_data = data.priceList[0].data;
						xArr = data.priceList[0].title;
					}
				   
				   var max=Math.max.apply(Math,series_data);
				   var min=Math.min.apply(Math,series_data);
				  // var max2=adjuestMaxOrMin(max,true);
				 //  var min2=adjuestMaxOrMin(min,false);
				   
				   
				   var myChart = echarts.init(document.getElementById("charContainer"));
				   var  option = {
						grid:{
						 top:20,
						 bottom:30,
						 right:0
						},
					   	visualMap: [{
					   	    show: false,
					   	    type: 'continuous',
					   	    min: 4113,
					   	    max: 4886
					   	}],
					   	tooltip: {
					   	        trigger: 'axis'
					   	    },
					       xAxis: {
					           type: 'category',
					           data: xArr
					       },
					   	/*title: [{
					   	    left: 'center',
					   	    text: 'HPB300钢筋[直径6～32mm]'
					   	}],*/
					       yAxis: {
					   		min:min,
					   		max:max,
					        type: 'value',
					   		//interval:155
					       },
					       series: [{
					           data: series_data,
					           type: 'line',
					   		label:{
					   			show:true
					   		}
					       }]
					   };

					   if (option && typeof option === 'object') {
					       myChart.setOption(option);
					   }
				   
				   
					
	 	        });	
			   
		  
			   
			   
		   },
		   
		   clChanged:function(){
			   
			   for(var i=0;i<app.clList.length;i++){
				   var item=app.clList[i];
				   if(item.id==app.selectedClId){
					   app.selectedCl=item;
					   app.renderClHistory(app.selectedClId);	
					   break;
				   }
			   }		   
		   },
		   getRate:function(analysisResult){
			   if(analysisResult.trend=='Decrease'){
				   return analysisResult.rate;
			   }else{
				   return "+"+analysisResult.rate;
			   }
			   
		   },
		  
		  },
		  computed: {
			
		  },
		 filters:{
				substr:function(input,length){
					if(!input){
						return "";
					}
					return input.substring(0,length);
				}
				
			}
		});
	
	
	
	 dataLoader={
		 loadGovPriceGcCL:function(categoryName,callback){
			 var postData={pageSize:8,categoryName:categoryName};	
			 var url='/price/gov/getGcPriceListByCategoryName.json';
			 layer.load();
			 ajaxGetData(url,postData,callback);			 
		 },	 
		 loadGovPrice:function(callback){
			 var postData={size:10};	
			 var url='/price/gov/getLatestGovMainMap.json';
			 ajaxGetData(url,postData,callback);
		},
		loadTgQuestion:function(callback){
			 var url='/cms/tgknow/getQuestionForTgHome.json';
			 ajaxGetData(url,{},callback);
		},
		loadProviceDe:function(callback){
			 //公路工程定额，公路养护定额，水运养护定额，市政建筑定额
			 var postData={menuBsIds:"001001002,001001001,001001003,001001005"};	
			 var url='/shareData/site/document/getProvinceDe2.json';
			 ajaxGetData(url,postData,callback);
		},
		loadHotDeList:function(callback){
			 var postData={menuBsIds:"001001002,001001001,001001003,001001005"};	
			 var url='/shareData/site/document/getHotDeList.json';
			 ajaxGetData(url,postData,callback);
		},
		loadLastestDeList:function(callback){
			 var postData={menuBsIds:"001001002,001001001,001001003,001001005"};	
			 var url='/shareData/site/document/getLastestDeList.json';
			 ajaxGetData(url,postData,callback);
		},
		loadXmzbList:function(callback){
			 var postData={roadLevels:"全部,高速公路,一级公路,一般公路二级,汽车专用二级,三级公路,四级公路"};	
			 var url='/myFiles/myEcCostFile/site/calculate/billZb/xmzbList2.json';
			 ajaxGetData(url,postData,callback);
		},
		loadShareComposePrice:function(callback){
			 var postData={roadLevels:"全部,高速公路,一级公路,一般公路二级,汽车专用二级,三级公路,四级公路"};	
			 var url='/shareData/composePrice/share/getRoadLevelMap.json';
			 ajaxGetData(url,postData,callback);
		}
	}
	
	dataLoader.loadGovPrice(function(data){
		 app.glGovMainList=data.gl;
		 app.jzGovMainList=data.jz;	
		 app.$nextTick(function () {
			 $(".cjhq-info").slide ({titCell:".hd a",mainCell:".bd"});	//材价行情-信息分类
			 $(".ul-scroll-8").slide({mainCell:".bd",autoPage:true,effect:"top",autoPlay:true,vis:9});	//材价行情-8条垂直切换

		 });
	});
	
	dataLoader.loadTgQuestion(function(data){
		if(data.item){
			app.questionList=data.item;
			for(var i=0;i<app.questionList.length;i++){
				app.questionList[i].url="/cms/tgknow/question-"+app.questionList[i].id+"/index.html";
			}
			app.$nextTick(function () {
				 $(".ul-scroll-6").slide({mainCell:".bd",autoPage:true,effect:"top",autoPlay:true,vis:6});	//材价行情-8条垂直切换
			 });
		}
		  
	});
	
	dataLoader.loadProviceDe(function(data){
		var menuBsIds=["001001002","001001001","001001003","001001005"];
		app.glProvinceDeList=data[menuBsIds[0]];
		app.glyhProvinceDeList=data[menuBsIds[1]];
		app.wateryhProvinceDeList=data[menuBsIds[2]];
		app.buildingProvinceDeList=data[menuBsIds[3]];		
	});
	
