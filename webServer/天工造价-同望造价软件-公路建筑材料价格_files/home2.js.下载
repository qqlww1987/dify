$(function() {
	
	var url = document.location.href;
	
	appMain();	
	
    bindEvents();
    
    setTimeout(function(){
        $(".cjhq-chart ul li.on")[0].click();

    }, 250);
    
  
	// 检查企业会员是否激活，异步登录TUC论坛
	loginInOperation([checkisCorpMemberAndNotActivate,syncLogin]);
	
});


function downloadWec(){
	 var url = "http://www.tgcost.com/asc/app/info.html?id=8a81a89f46462f5c0146464960f30080&type=soft";
	 window.open(url);
}


//ajax公共方法
function ajaxGetData(url,postData,sucessHandler){
	$.ajax({
		url : url,
		type : 'post',
		data:postData,
		dataType : 'json',
		timeout : 10000,
		async : true, //异步
		success : function(data) {	
			sucessHandler(data);
		},
		error:function(){
			console.log("ajax load data error from url:"+url);
			layer.close(layer.index);
		}		
	});		
}

