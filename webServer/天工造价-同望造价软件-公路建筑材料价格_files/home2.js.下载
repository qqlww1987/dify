	dataLoader.loadXmzbList(function(data){
		var roadLevels=["全部","高速公路","一级公路","一般公路二级","汽车专用二级","三级公路","四级公路"];
		var index=0;
		app.qdList=data[roadLevels[index]].qd;
		app.gsList=data[roadLevels[index]].gs;
		app.ysList=data[roadLevels[index]].ys;
		
		index++;
		app.hightSpeed_qdList=data[roadLevels[index]].qd;
		app.hightSpeed_gsList=data[roadLevels[index]].gs;
		app.hightSpeed_ysList=data[roadLevels[index]].ys;
		
		index++;
		app.L1_qdList=data[roadLevels[index]].qd;
		app.L1_gsList=data[roadLevels[index]].gs;
		app.L1_ysList=data[roadLevels[index]].ys;
		
		index++;
		app.L2_qdList=data[roadLevels[index]].qd;
		app.L2_gsList=data[roadLevels[index]].gs;
		app.L2_ysList=data[roadLevels[index]].ys;
		
		index++;
		app.L2sp_qdList=data[roadLevels[index]].qd;
		app.L2sp_gsList=data[roadLevels[index]].gs;
		app.L2sp_ysList=data[roadLevels[index]].ys;
		
		index++;
		app.L3_qdList=data[roadLevels[index]].qd;
		app.L3_gsList=data[roadLevels[index]].gs;
		app.L3_ysList=data[roadLevels[index]].ys;
		
		index++;
		app.L4_qdList=data[roadLevels[index]].qd;
		app.L4_gsList=data[roadLevels[index]].gs;
		app.L4_ysList=data[roadLevels[index]].ys;	
	    app.$nextTick(function () {
				$(".ul-scroll-6").slide({mainCell:".bd",autoPage:true,effect:"top",autoPlay:true,vis:6});	//项目案例-6条垂直切换，可通用		 
		});		
	});
	
	dataLoader.loadShareComposePrice(function(data){		
		var roadLevels=["全部","高速公路","一级公路","一般公路二级","汽车专用二级","三级公路","四级公路"];
		var index=0;
		app.share_qdList=data[roadLevels[index]].qd;
		app.share_gsList=data[roadLevels[index]].gs;
		app.share_ysList=data[roadLevels[index]].ys;
		
		index++;
		app.share_hightSpeed_qdList=data[roadLevels[index]].qd;
		app.share_hightSpeed_gsList=data[roadLevels[index]].gs;
		app.share_hightSpeed_ysList=data[roadLevels[index]].ys;
		
		index++;
		app.share_L1_qdList=data[roadLevels[index]].qd;
		app.share_L1_gsList=data[roadLevels[index]].gs;
		app.share_L1_ysList=data[roadLevels[index]].ys;
		
		index++;
		app.share_L2_qdList=data[roadLevels[index]].qd;
		app.share_L2_gsList=data[roadLevels[index]].gs;
		app.share_L2_ysList=data[roadLevels[index]].ys;
		
		index++;
		app.share_L2sp_qdList=data[roadLevels[index]].qd;
		app.share_L2sp_gsList=data[roadLevels[index]].gs;
		app.share_L2sp_ysList=data[roadLevels[index]].ys;
		
		index++;
		app.share_L3_qdList=data[roadLevels[index]].qd;
		app.share_L3_gsList=data[roadLevels[index]].gs;
		app.share_L3_ysList=data[roadLevels[index]].ys;
		
		index++;
		app.share_L4_qdList=data[roadLevels[index]].qd;
		app.share_L4_gsList=data[roadLevels[index]].gs;
		app.share_L4_ysList=data[roadLevels[index]].ys;	

	    app.$nextTick(function () {
				$(".ul-scroll-6").slide({mainCell:".bd",autoPage:true,effect:"top",autoPlay:true,vis:6});	//项目案例-6条垂直切换，可通用		 
		});		
		
	});
	
	
	
}
function bindEvents(){
	
	// 切换集合
	$(".iBanner").slide({mainCell:".bd ul",autoPlay:true,effect:"fold",interTime:5000});	//banner
    $(".F-cjhq").slide({titCell:".cjhq-hd li",mainCell:".cjhq-bd"}); 	//材价行情-总分类
    //$(".cjhq-chart").slide({titCell:".hd li",mainCell:".chartList"});	//材价行情-2级分类
    $(".cjhq-info").slide ({titCell:".hd a",mainCell:".bd"});	//材价行情-信息分类
    $(".F-xmal").slide({titCell:".xmal-hd h3",mainCell:".xmal-bd"}); 	//项目案例-总分类
    $(".f-cate-slide").slide(); 	//项目案例-总分类   
    
    
    $(".indexCate>li").mouseenter(function(){
    	var widths=[];
    	$(this).find(".lev2_title").each(function(){
    		widths.push($(this).width());
    	});
    	var maxWidth=Math.max.apply(Math,widths);
    	$(this).find(".lev2_title").width(maxWidth+"px");
    	
         console.log($(this).find(".lev2_title").length);
    });
    

}



//检查是否需要上传头像
function checkisCorpMemberAndNotActivate(){
	var a = Math.random(); 
	var url='/myFiles/corp/isCorpCreatorAndNotActivate.json'+'?rand='+a;
	$.ajax({
		url : url,
		type : 'post',
		dataType : 'json',
		timeout : 5000,
		async : true, //异步
		success : function(data) {
			if(data&&data.success){
			  	$(".popwin-corpMemberActivate").show();
			}    										
		},
		error:function(){

		}		
	});
}


function byPoint(version,position,remark){
	$.ajax({
	  	type: "post",
	      url: "/base/byPoint/saveBp.json",
	      data:{'position':position,'version':version,'remark':remark},
	      async: true,
	});	
}

function byPointMessage(version,position,remark){
	var dataProvince = $("#areaTit").text();
	$.ajax({
		type: "post",
		url: "/base/byPoint/saveBp.json",
		data:{'position':position,'version':version,'remark':remark,'dataProvince':dataProvince},
		async: true,
	});	
}

function byPointProvince(version,position,dataProvince,remark){
	$.ajax({
	  	type: "post",
	      url: "/base/byPoint/saveBp.json",
	      data:{'position':position,'version':version,'remark':remark,'dataProvince':dataProvince},
	      async: true,
	});	
}
